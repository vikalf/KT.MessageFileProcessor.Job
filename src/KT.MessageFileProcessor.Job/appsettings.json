{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
        "Default": "Information",
        "System": "Information",
        "Microsoft": "Information"
    },
    "CorrelationHeaderKey": "l5d-ctx-trace"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
        "Override": {
            "Microsoft": "Information",
            "System": "Information"
        }
    },
    "Enrich": [ "FromLogContext", "WithMachineName", "WithProcessId", "WithThreadId" ],
    "WriteTo": []
  },
  "AppSettings": {
    "AppName": "KT.MessageFileProcessor.Job"
  }
}